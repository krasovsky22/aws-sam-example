{
  "name": "aws-image-generation",
  "version": "1.0.0",
  "description": "This project contains source code and supporting files for a serverless application that you can deploy with the SAM CLI. It includes the following files and folders.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently npm:watch:*",
    "build": "npm run clean && webpack",
    "clean": "rimraf build",
    "clean-sam": "rm -rf .aws-sam",
    "lint": "eslint --ext .ts .",
    "test": "jest",
    "deploy": "sam deploy",
    "watch:webpack": "npm run build -- --watch",
    "watch:sam": "dotenv -- cross-var sam local start-api --host %HOST%"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@types/expect": "^24.3.0",
    "aws-lambda": "^1.0.6",
    "aws-sam-webpack-plugin": "^0.9.0",
    "aws-sdk": "^2.942.0",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^4.0.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "winston": "^3.3.3",
    "yaml-cfn": "^0.3.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.77",
    "@types/jest": "^26.0.23",
    "@types/mocha": "^8.2.2",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "concurrently": "^6.2.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "lint": "^0.7.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2"
  }
}
